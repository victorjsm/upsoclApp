<h1>List of users</h1>

<table>
	<tr>

			<th>rut</th>
			<th>name</th>
			<th>lastName</th>
			<th>email</th>
			<th>birthDate</th>
			<th>password</th>
		
	</tr>

	<% @users.each do |user| %>
		<tr>
			<TD><%= user.rut %></TD>
			<TD><%= user.name %></TD>
			<TD><%= user.lastName %></TD>
			<TD><%= user.email %></TD>
			<TD><%= user.birthDate %></TD>
			<TD><%= user.password %></TD>
			<TD><%= link_to 'Show', user_path(user)%></TD>
			<td><%= link_to 'Edit', edit_user_path(user) %></td>
			<td><%= link_to 'Destroy', user_path(user),
              		method: :delete,
              		data: { confirm: 'Are you sure?' } %></td>
		</tr>
	<%end%>
</table>
<%= link_to 'New user', new_user_path %>
